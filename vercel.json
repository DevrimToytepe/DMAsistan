{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    },
    {
      "source": "/(dashboard|conversations|contacts|automation|analytics|integrations|billing|settings|onboarding)(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/(.*)\\.(?:js|css|woff2?|png|jpg|jpeg|webp|svg|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/giris",
      "destination": "/giris.html"
    },
    {
      "source": "/kayit",
      "destination": "/kayit.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/konusmalar",
      "destination": "/conversations.html"
    },
    {
      "source": "/kisiler",
      "destination": "/contacts.html"
    },
    {
      "source": "/otomasyon",
      "destination": "/automation.html"
    },
    {
      "source": "/analitik",
      "destination": "/analytics.html"
    },
    {
      "source": "/entegrasyonlar",
      "destination": "/integrations.html"
    },
    {
      "source": "/ayarlar",
      "destination": "/settings.html"
    },
    {
      "source": "/faturalama",
      "destination": "/billing.html"
    },
    {
      "source": "/onboarding",
      "destination": "/onboarding.html"
    },
    {
      "source": "/offline",
      "destination": "/offline.html"
    }
  ]
}